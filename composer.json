{
    "name": "nepada/birth-number-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Czech birth number type for Doctrine.",
    "keywords": ["birth number", "value object", "doctrine", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/birth-number-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=8.1.0 <8.5",
        "doctrine/dbal": "^3.4@dev || ^4.0@dev",
        "nepada/birth-number": "^1.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.5.4",
        "mockery/mockery": "1.6.12",
        "phpstan/phpstan": "2.1.13",
        "phpstan/phpstan-strict-rules": "2.0.4",
        "phpstan/phpstan-nette": "2.0.3",
        "phpstan/phpstan-mockery": "2.0.0",
        "nepada/phpstan-nette-tester": "2.0.0",
        "spaze/phpstan-disallowed-calls": "4.5.0",
        "shipmonk/phpstan-rules": "4.1.1",
        "php-parallel-lint/php-parallel-lint": "1.4.0",
        "nepada/coding-standard": "7.14.1",
        "composer-runtime-api": "^2.0",
        "composer/semver": "3.4.3",
        "nette/utils": ">=3.2.7"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\BirthNumberDoctrine\\": "src/BirthNumberDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": false
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    }
}
