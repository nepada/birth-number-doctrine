{
    "name": "nepada/birth-number-doctrine",
    "type": "library",
    "license": "BSD-3-Clause",
    "description": "Czech birth number type for Doctrine.",
    "keywords": ["birth number", "value object", "doctrine", "nette"],
    "support": {
        "email": "petr@pada.cz",
        "issues": "https://github.com/nepada/birth-number-doctrine/issues"
    },
    "authors": [
        {
            "name": "Petr MorÃ¡vek",
            "email": "petr@pada.cz"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "doctrine/dbal": "^2.6@dev",
        "nepada/birth-number": "^1.0@dev"
    },
    "require-dev": {
        "nette/tester": "2.4.0",
        "mockery/mockery": "1.4.3",
        "phpstan/phpstan": "0.12.88",
        "phpstan/phpstan-strict-rules": "0.12.9",
        "pepakriz/phpstan-exception-rules": "0.11.7",
        "phpstan/phpstan-nette": "0.12.21",
        "nepada/phpstan-nette-tester": "0.3.1",
        "spaze/phpstan-disallowed-calls": "1.4.0",
        "phpstan/phpstan-mockery": "0.12.14",
        "php-parallel-lint/php-parallel-lint": "1.2.0",
        "nepada/coding-standard": "7.4.0"
    },
    "autoload": {
        "psr-4": {
            "Nepada\\BirthNumberDoctrine\\": "src/BirthNumberDoctrine/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NepadaTests\\": "tests/"
        }
    },
    "config": {
        "platform-check": false
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
